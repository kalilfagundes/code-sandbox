{
  "root": true,
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "lacussoft",
    "lacussoft/typescript"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "unix"],
    "no-console": "off",
    "no-magic-numbers": "off"
  }
}
